{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
	"headers": [
		{
			"source" : "**/*.@(js|html|css)",
			"headers": [
				{
					"key":"Cache-Control",
					"value":"No-Cache"
				}
			]
		}
	
	]
  }
}
